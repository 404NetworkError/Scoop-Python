{
    "version": "1.3.0",
    "homepage": "https://github.com/twolfson/restructuredtext-lint",
    "description": "reStructuredText linter.",
    "license": "Unlicense",
    "depends": "python-bucket-utils",
    "url": "https://github.com/twolfson/restructuredtext-lint/archive/1.3.0.zip",
    "hash": "9b1937a38384bd55f4ef7d8f79438571c4871a6f0d316f2e655fea113e63c4fb",
    "extract_dir": "restructuredtext-lint-1.3.0",
    "installer": {
        "script": [
            "if ($global) { install-python-package --global --quiet --dir $dir }",
            "else { install-python-package --quiet --dir $dir }",
            "if ($LASTEXITCODE) { exit 1 }"
        ]
    },
    "uninstaller": {
        "script": [
            "if ($global) { uninstall-python-package restructuredtext_lint --global --dir $dir --match '.*restructuredtext.lint.*|rst-lint.*' }",
            "else { uninstall-python-package restructuredtext_lint --dir $dir --match '.*restructuredtext.lint.*|rst-lint.*' }"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/twolfson/restructuredtext-lint/archive/$version.zip",
        "extract_dir": "restructuredtext-lint-$version"
    }
}
