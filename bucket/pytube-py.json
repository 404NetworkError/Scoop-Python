{
    "version": "10.7.0",
    "homepage": "https://python-pytube.readthedocs.io/",
    "description": "Utility for downloading YouTube videos.",
    "license": "MIT",
    "depends": "python-bucket-utils",
    "url": "https://github.com/nficano/pytube/archive/v10.7.0.zip",
    "hash": "a11b6ee8098cb4d48c5b69942979249489bbb0d51e698cb7b7a8d625b864f7ae",
    "extract_dir": "pytube-10.7.0",
    "installer": {
        "script": [
            "if ($global) { install-python-package --global --quiet --dir $dir }",
            "else { install-python-package --quiet --dir $dir }",
            "if ($LASTEXITCODE) { exit 1 }"
        ]
    },
    "uninstaller": {
        "script": [
            "if ($global) { uninstall-python-package $app --global --dir $dir }",
            "else { uninstall-python-package $app --dir $dir }"
        ]
    },
    "checkver": {
        "url": "https://api.github.com/repos/nficano/pytube/tags",
        "regex": "v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/nficano/pytube/archive/v$version.zip",
        "extract_dir": "pytube-$version"
    }
}
