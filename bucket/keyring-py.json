{
    "version": "21.2.1",
    "homepage": "https://github.com/jaraco/keyring",
    "description": "Provides an easy way to access the system keyring service from Python.",
    "license": "MIT",
    "depends": [
        "git",
        "python"
    ],
    "url": "https://raw.githubusercontent.com/TheRandomLabs/Scoop-Python/master/placeholder.txt",
    "hash": "8f5572e508c2cb95197e550f54c6bc03cfd4668ef3c9f1011ec3950037cb4b11",
    "extract_dir": "keyring-21.2.1",
    "installer": {
        "script": [
            "git clone --quiet -c advice.detachedHead=false --branch \"v$version\" https://github.com/jaraco/keyring.git \"$dir\\keyring\"",
            "Push-Location \"$dir\\keyring\"",
            "if ($global) { python setup.py --quiet install --record \"$dir\\installed_files.txt\" }",
            "else { python setup.py --quiet install --user --record \"$dir\\installed_files.txt\" }",
            "Pop-Location"
        ]
    },
    "uninstaller": {
        "script": [
            "Get-Content \"$dir\\installed_files.txt\" | Remove-Item -ErrorAction Ignore -Force",
            "if ($global) { $site_packages = \"$(Split-Path (scoop which python))\\lib\\site-packages\" }",
            "else { $site_packages = python -m site --user-site }",
            "Get-ChildItem \"$site_packages\\keyring-*-py*.egg\" | ForEach-Object { Remove-Item -ErrorAction Ignore -Force -Recurse $_ }"
        ]
    },
    "checkver": {
        "url": "https://api.github.com/repos/jaraco/keyring/tags",
        "regex": "v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/TheRandomLabs/Scoop-Python/master/placeholder.txt"
    }
}
