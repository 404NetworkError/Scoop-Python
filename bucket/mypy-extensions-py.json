{
    "version": "0.4.3",
    "homepage": "https://github.com/python/mypy_extensions",
    "description": "Extensions for mypy.",
    "license": "MIT",
    "depends": "mypy-py",
    "url": "https://github.com/python/mypy_extensions/archive/0.4.3.zip",
    "hash": "dcc5e1adcd378f76570deb7dd1678e9601f7de7c3dc0d29703203fa0eb1c5003",
    "extract_dir": "mypy_extensions-0.4.3",
    "installer": {
        "script": [
            "Push-Location $dir",
            "if ($global) { python setup.py --quiet install --record installed_files.txt }",
            "else { python setup.py --quiet install --user --record installed_files.txt }",
            "Pop-Location"
        ]
    },
    "uninstaller": {
        "script": [
            "Get-Content \"$dir\\installed_files.txt\" | Remove-Item -ErrorAction Ignore -Force",
            "if ($global) { $site_packages = \"$(Split-Path (scoop which python))\\lib\\site-packages\" }",
            "else { $site_packages = python -m site --user-site }",
            "Get-ChildItem \"$site_packages\\mypy_extensions-*-py*.egg\" | ForEach-Object { Remove-Item -ErrorAction Ignore -Force -Recurse $_ }"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/python/mypy_extensions/archive/$version.zip",
        "extract_dir": "mypy_extensions-$version"
    }
}
