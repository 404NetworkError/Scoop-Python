{
    "version": "0.34.2",
    "homepage": "https://github.com/pypa/wheel",
    "description": "Implementation of the Python wheel packaging standard.",
    "license": "MIT",
    "depends": "python-bucket-utils",
    "url": "https://github.com/pypa/wheel/archive/0.34.2.zip",
    "hash": "cf272e9aa1a0ed542056c70bc7d8294bcb2f591bdb7d2ef67b7ddc85fd4230b2",
    "extract_dir": "wheel-0.34.2",
    "installer": {
        "script": [
            "if ($global) { install-python-package --global --quiet --dir $dir }",
            "else { install-python-package --quiet --dir $dir }",
            "if ($LASTEXITCODE) { exit 1 }"
        ]
    },
    "uninstaller": {
        "script": [
            "if ($global) { uninstall-python-package $app --global --dir $dir }",
            "else { uninstall-python-package $app --dir $dir }"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/pypa/wheel/archive/$version.zip",
        "extract_dir": "wheel-$version"
    }
}
